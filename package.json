    "moment": "^2.14.1",
    "nlp-syllables": "0.0.5",
    "nlp-syllables-async": "0.0.1",
    "nlp_compromise": "^6.5.0",
    "scrape-url": "^3.0.0"
  },
  "devDependencies": {
    "standard": "^7.0.1"
  },
  "standard": {
    "globals": [
      "atom",
