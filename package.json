{
  "name": "syllable",
  "main": "./lib/index",
  "version": "0.0.1",
  "description": "An Atom package that groups words by their number of syllables",
  "keywords": [
    "linguistics",
    "nlp",
    "language"
  ],
  "repository": "https://github.com/partials-dev/syllable",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationCommands": {
